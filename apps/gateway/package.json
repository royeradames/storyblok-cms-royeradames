{
  "name": "gateway",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun scripts/seed-preview-user.ts",
    "docker:up": "docker compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker compose -f docker/docker-compose.yml down",
    "docker:logs": "docker compose -f docker/docker-compose.yml logs -f",
    "storyblok:seed:generate": "bun scripts/storyblok/generate-seed.ts",
    "storyblok:seed:push": "bun scripts/storyblok/push-seed.ts",
    "storyblok:seed": "bun run storyblok:seed:generate && bun run storyblok:seed:push",
    "storyblok:seed:templates": "bun run scripts/storyblok/seed-templates.ts",
    "storyblok:migrate:data-mapping": "bun run scripts/storyblok/migrate-flat-to-data-mapping.ts",
    "storyblok:migrate:premade-component-keys": "bun run scripts/storyblok/migrate-premade-component-keys.ts",
    "storyblok:migrate:border-color-theme": "bun run scripts/storyblok/migrate-border-color-to-theme-fields.ts",
    "storyblok:migrate-flex-options-to-styles": "bun scripts/storyblok/migrate-flex-options-to-styles.ts",
    "storyblok:migrate-flex-breakpoint-to-styles-breakpoint": "bun scripts/storyblok/migrate-flex-breakpoint-to-styles-breakpoint.ts",
    "storyblok:migrate-flex-to-container": "bun scripts/storyblok/migrate-flex-to-container.ts",
    "storyblok:delete-component": "bun scripts/storyblok/delete-component.ts",
    "styles:safelist": "bun scripts/generate-styles-safelist.ts"
  },
  "dependencies": {
    "@repo/ui": "workspace:*",
    "@repo/db": "workspace:*",
    "drizzle-orm": "^0.44.2",
    "postgres": "^3.4.7",
    "@repo/auth": "workspace:*",
    "@repo/shared-cms": "workspace:*",
    "@storyblok/react": "^4.6.0",
    "@tanstack/react-query": "^5.80.7",
    "next": "^15.3.3",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "uploadthing": "^7.7.1",
    "@uploadthing/react": "^7.3.1"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@tailwindcss/postcss": "4.1.18",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.4",
    "postcss": "^8.5.4",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0",
    "typescript": "^5.9.3"
  }
}
